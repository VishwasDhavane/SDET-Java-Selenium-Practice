package interviewpractice;

public class PrimeNumberCheck {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int p = 17;
		int count=0;
		
		for(int i=1;i<=p;i++)
		{
			if(p%i==0 )
			{
				count++;
			}
		}
		
		if(count>2)
		{
			System.out.println("it is  a prime number");
		}
		else
		{
			System.out.println("it is not a prime");
		}

	}

}
