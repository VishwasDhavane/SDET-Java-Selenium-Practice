package leetcodepractice;

public class ReverseWordsInAString {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String s = "the sky is blue";
	    String[] g=s.split(" ");
	    String a="";
	    
	    for(int i=g.length-1;i>=0;i--)
	    {
	    	a= a + g[i] + " ";
	    }
	    
	    System.out.println(a);

	}

}
