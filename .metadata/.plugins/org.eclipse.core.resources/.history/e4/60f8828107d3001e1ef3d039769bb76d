package arraypractice;

public class practice {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		  String s1="vishwas";  //7
	        String s2="";

	        for(int i=s1.length()-1;i>0;i--)
	        {
	            char ch =s1.charAt(i);
	            s2= s2 + ch + " ";
	             }

	        System.out.println(s2);

	    }

	}


